<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="10.0"
    xmlns="http://javafx.com/javafx/17.0.1"
    xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.WelcomeController">
    <Text text="Welcome" style="-fx-font-size: 24px;"/>
    <Button fx:id="registerButton" text="Register" onAction="#handleRegister" />
    <Button text="Login" fx:id="loginButton" onAction="#handleLogin"/>
</VBox>
